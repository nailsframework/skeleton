    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "@nailsframework/nails": "^1.0.20",
    "awesome-typescript-loader": "^5.2.1",
    "install": "^0.13.0",
    "source-map-loader": "^0.2.4",
    "tslint": "^5.20.1"
  }
}
